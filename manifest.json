{
  "name": "COVID-Free Browsing",
  "version": "0.1",
  "description": "CF Browsing attempts to hide any COVID-19 related content to allow more peace while being on the internet",
  "permissions": ["activeTab", "declarativeContent", "storage"], 
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
